{
  "hosting": {
    "site": "adultjapans",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        // redirect all calls to the function called "api"
        "source": "**",
        "function": "api/index.php"
      }
    ]
  }
}